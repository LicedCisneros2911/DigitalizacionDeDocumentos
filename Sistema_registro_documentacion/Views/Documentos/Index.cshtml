@model IEnumerable<Sistema_registro_documentacion.Models.Documento>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<h1 style="text-align:center;">Lista de Informes</h1>
<br />
<br />
<p>
    <a class="btn btn-primary" asp-action="Create">Agregar informe</a>
</p>
@{
    List<SelectListItem> ListFolder = new List<SelectListItem>();
    ListFolder.Add(new SelectListItem() { Text = "TODOS", Value = "0" });
    ListFolder.AddRange(ViewBag.folderList);
    List<SelectListItem> ListTipo = new List<SelectListItem>();
    ListTipo.Add(new SelectListItem() { Text = "TODOS", Value = "0" });
    ListTipo.AddRange(ViewBag.tipoList);
    List<SelectListItem> ListFecha = new List<SelectListItem>(ViewBag.gestionList);
}
@using (Html.BeginForm("Index", "Documentos"))
{
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                <div class="form-group">
                    <label class="custom-control-label"><b>Folder</b></label>
                    @Html.DropDownList("Folder", ListFolder)
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <label class="custom-control-label"><b>Tipo</b></label>
                    @Html.DropDownList("Tipo", ListTipo)
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <label class="custom-control-label"><b>Gestion</b></label>
                    @Html.DropDownList("Fecha", ListFecha)
                </div>
            </div>
            <div class="col-sm-1">
                <button type="submit" style="background-color:black;color:white;">Buscar</button>
            </div>
        </div>
    </div>
}
<br />
<br />

<h4 style="text-align:center;">Cantidad de registros: @ViewBag.Cantidad</h4>

<table id="tableDocumento" style="overflow-x: auto;" class="table table-striped  table-bordered dt-responsive nowrap" cellspacing="0">
    <thead style="background-color:black; color:white;">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.folder)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.titulo)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.cite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.de)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.via)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.a)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ref)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.descripcion)
                </th>*@
            @*<th>
                    @Html.DisplayNameFor(model => model.foto)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.filefoto)
            </th>
            @if (User.IsInRole("Administrador") || User.IsInRole("Editor"))
            {
                <th style="width:260px;">Acciones</th>
            }
        </tr>
    </thead>
    <tbody id="documentacion">
        @foreach (var item in Model)
        {
            <tr>
                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.formulario_Tipo.nombre_tipo)
                </td>
                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.formulario_Folder.nombre_folder)
                </td>
                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.cite)
                </td>

                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.de)
                </td>
                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.a)
                </td>
                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.Ref)
                </td>

                <td style="word-wrap: break-word; max-width: 200px; white-space: inherit; ">
                    @Html.DisplayFor(modelItem => item.fecha)
                </td>

                @if (item.foto != null)
                {
                    <td>
                        @Html.ActionLink("Ver Documento", "ShowPDF", "Documentos", new { id = item.id })
                    </td>
                }
                else
                {
                    <td>
                        No existe el documento
                    </td>
                }
                @if (User.IsInRole("Administrador") || User.IsInRole("Editor"))
                {
                    <td>
                        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@item.id"><i class="bi bi-pencil-square"></i></a>
                        <a class="btn btn-info" asp-action="Details" asp-route-id="@item.id"><i class="bi bi-file-earmark-text-fill"></i></a>
                        <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.id"><i class="bi bi-trash-fill"></i></a>
                    </td>
                }
            </tr>
        }
    </tbody>
</table>


@section Scripts
{
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>

    @*<script src="~/lib/datatables-buttons/js/dataTables.buttons.min.js"></script>*@
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/2.0.0/js/dataTables.buttons.min.js"></script>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/2.0.0/js/buttons.html5.min.js"></script>

    <script>
    window.onload = function () {
        var v = '@ViewBag.msg';
        var msg = v;
        if (msg != null && msg != "") {
            if (msg == "Exito") {
                $('#myModalSuccess').modal('show');
            }
            else {
                $('#myModalError').modal('show');
            }
        }
    }
    </script>


    <script>
		$(document).ready(function () {
			var v = '@ViewBag.Folder';
			var strFolder = v;
            if (strFolder == "") {strFolder = "Reporte Documentos"}

			$('#tableDocumento').DataTable({
				"language": {
					"search": "Buscar...",
					"lengthMenu": "Mostrar _MENU_ registros por página",
					"zeroRecords": "No se encontró ningún registro",
					"info": "Mostrando página _PAGE_ de _PAGES_",
					"infoEmpty": "No se encontró ningún registro",
					"infoFiltered": "(Se filtró de _MAX_ registros totales)",
					"paginate": {
						"previous": "Anterior",
						"next": "Siguiente",
						"first": "Principio",
						"last": "Fin"
					},
				},
				dom: 'Blfrtip',
				"autoWidth": true,
				buttons: [
					{
						extend: 'pdfHtml5',
						orientation: 'portrait',
						pageSize: 'letter',

						exportOptions: {
							columns: [0, 1, 2, 3, 5],
							search: 'applied',
							order: 'applied'
						},

						title: 'Reporte Documentos',

						customize: function (doc) {
							doc.content[1].table.widths =
								Array(doc.content[1].table.body[0].length + 1).join('*').split('');

							doc.content.splice(0, 1);

							var logo = 'data:image/jpeg;base64,';

							doc.pageMargins = [25, 85, 25, 30];

							doc.styles.tableHeader.fontSize = 14;
							doc.defaultStyle.fontSize = 10;

							doc['header'] = (function () {
								return {
									columns: [
										{
											image: logo,
											width: 36,
											alignment: 'left'
										},
										{
											alignment: 'center',
											italics: true,
											text: strFolder,
											fontSize: 18,
										},
									],
									margin: 30
								}
							});


							doc['footer'] = (function (page, pages) {
								return {
									columns: [
										{
											alignment: 'right',
											text: [
												{ text: page.toString(), italics: true },
												' de ',
												{ text: pages.toString(), italics: true }
											]
										}],
									margin: [30, 0]
								}
							});

							var objLayout = {};
							objLayout['hLineWidth'] = function (i) { return .5; };
							objLayout['vLineWidth'] = function (i) { return .5; };
							objLayout['hLineColor'] = function (i) { return '#aaa'; };
							objLayout['vLineColor'] = function (i) { return '#aaa'; };
							objLayout['paddingLeft'] = function (i) { return 4; };
							objLayout['paddingRight'] = function (i) { return 4; };
							doc.content[0].layout = objLayout;
						}
					},
				]
			});
		});

    </script>

}
